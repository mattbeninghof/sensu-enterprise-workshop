{
  "checks": {
    "syslog_config_checksum": {
      "command": "/opt/sensu/embedded/bin/check-checksums.rb -f log_file  -h 05cda4cb5dafb50c8712a1b4cfe893b2e08b335b29f9a102a8aee061f85f364b",
      "interval": 60,
      "subscribers": ["compliance"],
      "publish": true,
      "contacts": ["grc-team"],
      "type": "standard",
      "ttl": 180,
      "handlers": ["slack"],
      "hooks": {
        "warning": {
          "command": "",
          "timeout": 10
        },
        "critical": {
          "command": "",
          "timeout": 10
        }
      },
      "occurrences": 1,
      "refresh": 1,
      "plugin": {
        "source": "https://github.com/sensu-plugins/sensu-plugins-filesystem-checks",
        "install": "sudo sensu-install -p filesystem-checks"
      }
    },
    "syslog_config_pattern": {
      "command": "perl plugins/check_file_content.pl -f log_file -i config -n 0",
      "interval": 60,
      "subscribers": ["compliance"],
      "publish": true,
      "contacts": ["grc-team"],
      "type": "standard",
      "ttl": 180,
      "handlers": ["slack"],
      "hooks": {
        "warning": {
          "command": "",
          "timeout": 10
        },
        "critical": {
          "command": "",
          "timeout": 10
        }
      },
      "occurrences": 1,
      "refresh": 1,
      "plugin": {
        "source": "https://exchange.nagios.org/directory/Addons/Log-File-Management/check_file_content/details",
        "install": "curl -o check_file_content.pl -L 'https://exchange.nagios.org/components/com_mtree/attachment.php?link_id=4508&cf_id=24'"
      }
    },
    "syslog_process": {
      "command": "/opt/sensu/embedded/bin/check-process.rb -p syslog",
      "interval": 60,
      "subscribers": ["compliance"],
      "publish": true,
      "contacts": ["grc-team"],
      "type": "standard",
      "ttl": 180,
      "handlers": ["slack"],
      "hooks": {
        "warning": {
          "command": "",
          "timeout": 10
        },
        "critical": {
          "command": "",
          "timeout": 10
        }
      },
      "occurrences": 1,
      "refresh": 1,
      "plugin": {
        "source": "https://github.com/sensu-plugins/sensu-plugins-process-checks",
        "install": "sudo sensu-install -p process-checks"
      }
    }
  }
}
